{
    "name": "andrewreborn/mediatoolkit-php",
    "description": "A MediaToolkit SDK written in PHP that works with the MediaToolkit ReST API",
    "type": "library",
    "authors": [{
        "name": "Andrea Tantimonaco",
        "email": "info@andreatantimonaco.me"
    }],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "nesbot/carbon": "^1.22",
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Mediatoolkit\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/AndrewReborn/mediatoolkit-php"
    }],
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "vlucas/phpdotenv": "^2.0"
    }
}